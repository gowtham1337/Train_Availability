{
  "manifest_version": 2,
  "name": "Train Availability",
  "description": "Find availability of all trains at one go on indianrail.gov.in",
  "version": "1.0",

  "permissions": [
    "http://www.indianrail.gov.in/*"
  ],
  
  "icons": { "16": "images/logo16.png",
           "48": "images/logo48.png",
          "128": "images/logo128.png" },
		  
  "content_scripts": [
    {
      "matches": ["http://www.indianrail.gov.in/*"],
      "js": ["js/jquery.min.js","js/script.js"]
    }
  ]

}